package cpract;

import java.util.Scanner;

public class Cparact {
  
    public void lPrint(int length){
        for (int i = 0; i <length ; i++) {
            if(i==length-1){
                for (int j = 0; j <length ; j++) {


            System.out.print("*");
        }}
            else{
                System.out.println("*");
            }
        }
    }
    public static void main(String... pankajSharma ) {
        Scanner input = new Scanner(System.in);
        System.out.println("Plase enter length of charecter");
        int length = input.nextInt();
        Cparact obj = new Cparact();
    obj.lPrint(length);

    }

    }


